{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "types": ["expo-router", "node"],
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx", "app", "src", "lib", "lib"],
  "exclude": ["node_modules", "dist", "build"]
}
